      <nodes>
        <node id="2" posx="868.87890625" posy="-320.6328125" width="518" height="112" side="right" fold="false" treesize="112" color="#f37329" colorroot="false" summarized="false" layout="Horizontal" group="false">
          <style branchmargin="100" branchradius="25" linktype="curved" linkwidth="4" linkarrow="false" linkdash="longdash" nodeborder="underlined" nodewidth="547" nodeborderwidth="4" nodefill="false" nodemargin="8" nodepadding="6" nodefont="Sans 11" nodemarkup="true"/>
          <nodename maxwidth="547">
            <text data="What if we want to have a `String` with something more complicated?&#10;For example: &#10;`&quot;Java Study Guide&quot;&#10;   by Jeanne &amp; Scott`"/>
          </nodename>
          <nodenote></nodenote>
          <nodes>
            <node id="1" posx="1486.87890625" posy="-310.1328125" width="591" height="91" side="right" fold="false" treesize="98" color="#f37329" colorroot="false" summarized="false" layout="Horizontal" group="false">
              <style branchmargin="100" branchradius="25" linktype="curved" linkwidth="4" linkarrow="false" linkdash="longdash" nodeborder="underlined" nodewidth="547" nodeborderwidth="4" nodefill="false" nodemargin="8" nodepadding="6" nodefont="Sans 11" nodemarkup="true"/>
              <nodename maxwidth="566.337890625">
                <text data="The syntax `\&quot;` lets you say you want a `&quot;` rather than to end the `String`, and `\n` says you want a new line. Both of these are called _escape characters_ because the backslash provides a special meaning. "/>
              </nodename>
              <nodenote></nodenote>
              <nodes>
                <node id="3" posx="2177.87890625" posy="-313.6328125" width="603" height="49" side="right" fold="false" treesize="49" color="#f37329" colorroot="false" summarized="false" layout="Horizontal" group="false">

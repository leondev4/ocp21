                  <nodename maxwidth="598.544921875">
                    <text data="`String eyeTest = &quot;\&quot;Java Study Guide\&quot;\n    by Jeanne &amp; Scott&quot;;`"/>
                  </nodename>
                  <nodenote></nodenote>
                </node>
                <node id="4" posx="2177.87890625" posy="-264.6328125" width="146" height="49" side="right" fold="false" treesize="49" color="#f37329" colorroot="false" summarized="false" layout="Horizontal" group="false">

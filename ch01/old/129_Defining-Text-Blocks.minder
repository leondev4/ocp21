          <nodename maxwidth="547">
            <text data="`String textBlock = &quot;&quot;&quot;&#10;    &#x2502;&quot;Java Study Guide&quot;&#10;    &#x2502;   by Jeanne &amp; Scot&quot;&quot;&quot;;&#10;`"/>
          </nodename>
          <nodenote></nodenote>
          <nodes>
            <node id="7" posx="1248.87890625" posy="-159.6328125" width="501" height="70" side="right" fold="false" treesize="70" color="#c6262e" colorroot="false" summarized="false" layout="Horizontal" group="false">
              <style branchmargin="100" branchradius="25" linktype="curved" linkwidth="4" linkarrow="false" linkdash="longdash" nodeborder="underlined" nodewidth="547" nodeborderwidth="4" nodefill="false" nodemargin="8" nodepadding="6" nodefont="Sans 11" nodemarkup="true"/>
              <nodename maxwidth="547">
                <text data="A text block starts and ends with three double quotes (`&quot;&quot;&quot;`), and the&#10;contents don&#x2019;t need to be escaped."/>
              </nodename>
              <nodenote></nodenote>

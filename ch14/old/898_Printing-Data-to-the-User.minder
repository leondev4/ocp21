          <nodename maxwidth="446.001953125">
            <text data="Java includes two `PrintStream` instances for providing information to the user: `System.out` and `System.err`. "/>
          </nodename>
          <nodenote></nodenote>
        </node>
        <node id="2" posx="1174.318359375" posy="388" width="459" height="70" side="right" fold="false" treesize="70" color="#f37329" colorroot="false" summarized="false" layout="Horizontal" group="false">

              <nodename maxwidth="654">
                <text data="When directories are copied, the copy is _shallow_. A _shallow copy_ means that the files and&#10;subdirectories within the directory are not copied. A _deep copy_ means that the entire tree&#10;is copied, including all of its content and subdirectories. A deep copy typically requires recursion, where a method calls itself."/>
              </nodename>
              <nodenote></nodenote>
              <nodes>
                <node id="20" posx="2175.5651041666665" posy="571.5" width="676" height="232" side="right" fold="false" treesize="232" color="#f9c440" colorroot="false" summarized="false" layout="Horizontal" group="false">
